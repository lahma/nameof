{
  "name": "@typescript-nameof/test",
  "private": true,
  "type": "module",
  "description": "A project for testing the functionality of the `nameof` plugin.",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "homepage": "https://github.com/typescript-nameof/nameof#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescript-nameof/nameof.git",
    "directory": "packages/test"
  },
  "bugs": {
    "url": "https://github.com/typescript-nameof/nameof/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "npm --prefix ./ttypescript-workspace install && npm --prefix ./webpack-workspace install && npm run patch",
    "patch": "ts-patch i",
    "lint": "eslint --max-warnings 0 ./scripts ./src .eslintrc.cjs",
    "lint-ide": "npm run lint || exit 0",
    "pretest": "copyfiles -u 1 ./src/{index.ts,jest.test.ts} ./src/test",
    "test:ts-patch": "npm run pretest && tsc -b tsconfig.test.json --watch",
    "test:ttsc": "npm run pretest && cd ./ttypescript-workspace && npx ttsc -b . --watch",
    "test:webpack": "npm run pretest && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" webpack --watch",
    "test:ts-jest": "npm run pretest && jest --watch",
    "test:babel-plugin": "npm run pretest && babel ./src/test --extensions \".ts\" --out-dir ./babel --watch"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^4.1.0",
    "@manuth/package-json-editor": "^3.1.1",
    "@manuth/tsconfig": "^4.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/git-branch": "^2.0.2",
    "@types/mocha": "file:../types",
    "@types/node": "^20.5.1",
    "@types/npm-which": "^3.0.1",
    "eslint": "^8.41.0",
    "fs-extra": "^11.1.1",
    "git-branch": "^2.0.1",
    "globby": "^13.1.4",
    "mocha": "^10.2.0",
    "npm-which": "^3.0.1",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-patch": "3.0.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^29.5.3",
    "@types/nameof": "file:../types",
    "cross-env": "^7.0.3",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1"
  }
}
