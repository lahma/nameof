{
  "name": "@typescript-nameof/babel",
  "version": "0.0.1",
  "type": "module",
  "description": "nameof in TypeScript for babel.",
  "license": "MIT",
  "keywords": [
    "nameof",
    "typescript",
    "transforms",
    "babel",
    "babel-plugin-macros"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "./macro": {
      "types": "./macro/index.d.cts",
      "default": "./macro/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescript-nameof/nameof.git",
    "directory": "packages/babel"
  },
  "scripts": {
    "clean": "rimraf dist &&  tsc --b --clean",
    "build": "tsc --b",
    "test": "npm run build && mocha",
    "dopublish": "npm run install && npm run build && echo \"Run: npm publish --otp\""
  },
  "dependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.18.3",
    "@types/babel-plugin-macros": "^3.1.0",
    "@typescript-nameof/common": "^0.0.1",
    "@typescript-nameof/common-types": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.18.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.3",
    "@typescript-nameof/tests-common": "*",
    "babel-plugin-macros": "^3.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "babel-plugin-macros": "^3.1.0"
  }
}
