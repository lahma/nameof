{
    "name": "@typescript-nameof/common-types",
    "version": "0.0.1",
    "description": "Provides Common Types for @typescript-nameof Packages",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "types": "./lib/index.d.ts",
    "scripts": {
        "build": "tsc --build",
        "clean": "npm run build -- --clean"
    },
    "devDependencies": {
        "tsd": "^0.28.1"
    },
    "tsd": {
        "directory": "./type-tests",
        "compilerOptions": {
            "paths": {
                "@typescript-nameof/*": [
                    "../../*/dist",
                    "../../*/lib"
                ]
            }
        }
    }
}
