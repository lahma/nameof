{
  "name": "@typescript-nameof/nameof",
  "version": "0.0.1",
  "description": "Allows the Use of nameof in TypeScript",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "clean": "rimraf dist && tsc --b --clean",
    "build": "tsc --b",
    "build-test": "tsc --build && npm run --silent copy-test-files",
    "test": "npm run build-test && tsd && mocha",
    "test:debug": "npm run build-test && tsd && mocha",
    "copy-test-files": "rimraf temp && copyfiles -u 2 \"./src/tests/testFiles/**/*{.js,.txt}\" \"./temp\"",
    "dopublish": "npm run test && npm run build && echo \"run: npm publish --otp\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescript-nameof/nameof.git",
    "directory": "packages/nameof"
  },
  "keywords": [
    "nameof",
    "typescript",
    "transformer",
    "custom-transformer"
  ],
  "author": "The typescript-nameof Team (https://github.com/typescript-nameof",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typescript-nameof/nameof/issues"
  },
  "homepage": "https://github.com/typescript-nameof/nameof#readme",
  "peerDependencies": {
    "typescript": "*"
  },
  "dependencies": {
    "@typescript-nameof/common": "^0.0.1",
    "glob": "^9.2.1",
    "ts-patch": "^2.1.0"
  },
  "devDependencies": {
    "@tsd/typescript": "npm:nop@^1.0.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@typescript-nameof/tests-common": "*",
    "conditional-type-checks": "^1.0.6",
    "copyfiles": "^2.4.1",
    "mocha": "^10.2.0",
    "rimraf": "^4.3.1",
    "ts-jest": "^29.0.5",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typescript": "^4.9.5",
    "upath": "^2.0.1"
  },
  "tsd": {
    "directory": "./src/tests/type-tests"
  }
}
