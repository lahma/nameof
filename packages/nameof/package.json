{
  "name": "@typescript-nameof/nameof",
  "version": "0.0.1",
  "type": "module",
  "description": "Allows the use of `nameof()` calls in TypeScript projects.",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "nameof",
    "typescript",
    "transformer",
    "custom-transformer"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.cts",
      "default": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.cts",
  "homepage": "https://github.com/typescript-nameof/nameof#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescript-nameof/nameof.git",
    "directory": "packages/nameof"
  },
  "bugs": {
    "url": "https://github.com/typescript-nameof/nameof/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "npm run --prefix ../.. build",
    "test": "tsd && mocha"
  },
  "dependencies": {
    "@typescript-nameof/common": "^0.0.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "ts-patch": "^3.0.2"
  },
  "devDependencies": {
    "@manuth/package-json-editor": "^3.1.1",
    "@manuth/temp-files": "^3.0.2",
    "@manuth/tsconfig": "^4.0.1",
    "@tsd/typescript": "npm:nop@^1.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.3",
    "@typescript-nameof/tests-common": "*",
    "conditional-type-checks": "^1.0.6",
    "copyfiles": "^2.4.1",
    "mocha": "^10.2.0",
    "rimraf": "^4.3.1",
    "ts-jest": "^29.1.1",
    "ts-morph": "^17.0.1",
    "ts-nameof-proxy": "^0.1.14514",
    "ts-node": "^10.9.1",
    "tsd": "^0.28.1",
    "typescript": "^5.2.2",
    "upath": "^2.0.1"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "tsd": {
    "directory": "./type-tests"
  }
